{
  "name": "@valos/vault",
  "valaa": {
    "type": "vault",
    "domain": "kernel"
  },
  "version": "0.30.0-prerelease",
  "description": "Valaa common infrastructure tools and libraries monorepo.",
  "license": "MIT",
  "author": "Valaa Technologies Ltd",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "vlm": "vlm ",
    "start": "vlm revealer-dev-server ",
    "dev": "TARGET_ENV=local vlm revealer-dev-server ",
    "prod": "TARGET_ENV=production vlm revealer-dev-server ",
    "clean": "lerna clean --yes",
    "eslint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run eslint",
    "test": "jest",
    "build": "webpack ",
    "package-assemble": "vlm package-assemble ",
    "package-publish": "vlm package-publish "
  },
  "devDependencies": {
    "@valos/toolset-revealer": ">0.30.0-prerelease",
    "@valos/toolset-vault": ">0.30.0-prerelease",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "valma": ">0.30.0-prerelease"
  }
}
