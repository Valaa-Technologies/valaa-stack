{
  "name": "@valos/vault",
  "version": "0.30.0-prerelease",
  "description": "Valaa common infrastructure tools and libraries monorepo.",
  "license": "MIT",
  "author": "Valaa Technologies Ltd",
  "private": false,
  "valaa": {
    "type": "toolset",
    "domain": "kernel"
  },
  "scripts": {
    "start": "valma-revelations-dev-server ",
    "dev": "TARGET_ENV=local valma-revelations-dev-server ",
    "prod": "TARGET_ENV=production valma-revelations-dev-server ",
    "eslint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run eslint",
    "test": "jest",
    "test-e2e": "karma start",
    "build": "webpack ",
    "assemble-packages": "NODE_ENV=assemble-packages node script/valma-assemble-packages ",
    "publish-packages": "NODE_ENV=assemble-packages node script/valma-publish-packages "
  },
  "bin": {
    "valma-create-authollery": "bin/valma-create-authollery.js"
  },
  "jest": {
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      ".*/node_modules",
      ".*/schema",
      ".*/test"
    ],
    "verbose": true,
    "testRegex": "packages.*\\.test\\.js$",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/packages/tools/mocks/styleMock.js"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.26.1",
    "postcss-loader": "^2.0.8",
    "shelljs": "^0.8.2",
    "style-loader": "^0.13.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "webpack": "^3.8.1",
  },
  "devDependencies": {
    "@valos/revelations-dev-server": "file:packages/revelations-dev-server",
    "@valos/core": "file:packages/core",
    "@valos/prophet": "file:packages/prophet",
    "@valos/script": "file:packages/script",
    "@valos/tools": "file:packages/tools",
    "acorn": "^5.0.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "base64-js": "^1.2.3",
    "brace": "^0.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "fake-indexeddb": "^2.0.3",
    "flow": "^0.2.3",
    "flow-bin": "^0.36.0",
    "graphql": "^0.6.2",
    "gulp": "^3.9.1",
    "gulp-command": "^0.1.2",
    "immutable": "~3.7.4",
    "jasmine-core": "^2.8.0",
    "jest": "^21.1.0",
    "jss": "^8.1.0",
    "jss-css": "^3.0.0",
    "jss-preset-default": "^3.0.0",
    "jssha": "^2.3.1",
    "jstransform": "^11.0.3",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "lodash.isequal": "^4.5.0",
    "object-hash": "^1.1.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-ace": "^5.8.0",
    "react-contextmenu": "^2.6.5",
    "react-dom": "^16.2.0",
    "react-keydown": "^1.6.9",
    "react-tap-event-plugin": "^3.0.2",
    "reqwest": "^2.0.5",
    "text-encoding": "^0.6.4",
    "url-search-params": "^0.10.0",
    "uuid": "^2.0.2",
    "valma": "file:packages/valma"
  }
}
