{
  "name": "@valos/vault",
  "version": "0.30.0-prerelease",
  "description": "Valaa common infrastructure tools and libraries monorepo.",
  "license": "MIT",
  "author": "Valaa Technologies Ltd",
  "private": false,
  "valaa": {
    "type": "vault",
    "domain": "kernel"
  },
  "scripts": {
    "vlm": "vlm ",
    "start": "vlm revelations-dev-server ",
    "dev": "TARGET_ENV=local vlm revelations-dev-server ",
    "prod": "TARGET_ENV=production vlm revelations-dev-server ",
    "eslint": "eslint .",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "npm run eslint",
    "test": "jest",
    "test-e2e": "karma start",
    "build": "webpack ",
    "assemble-packages": "vlm assemble-packages ",
    "create-authollery": "vlm create-authollery ",
    "publish-packages": "vlm publish-packages ",
    "postinstall": "lerna bootstrap --hoist"
  },
  "bin": {},
  "jest": {
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      ".*/node_modules",
      ".*/schema",
      ".*/test"
    ],
    "verbose": true,
    "testRegex": "packages.*\\.test\\.js$",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/packages/tools/mocks/styleMock.js"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "@valos/revelations-dev-server": "file:packages/revelations-dev-server",
    "@valos/valma-toolset-authollery": "file:packages/valma-toolset-authollery",
    "@valos/valma-toolset-vault": "file:packages/valma-toolset-vault",
    "autoprefixer": "^8.6.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-root-import": "^5.1.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.26.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.36.0",
    "gulp": "^3.9.1",
    "gulp-command": "^0.1.2",
    "jasmine-core": "^2.8.0",
    "jest": "^21.1.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "postcss-loader": "^2.0.8",
    "shelljs": "^0.8.2",
    "style-loader": "^0.13.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "valma": "file:packages/valma",
    "webpack": "^3.8.1"
  }
}
