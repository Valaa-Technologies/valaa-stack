              "@type": "VDoc:Paragraph",
              "VDoc:content": [
                {
                  "VDoc:q": true,
                  "VDoc:content": [
                    [
                      "A ValOS Event ",
                      {
                        "@type": "VDoc:CharacterData",
                        "VDoc:content": [
                          "event"
