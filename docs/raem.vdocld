          "owl:maxCardinality": 1
        },
        "valos:isOwned": true,
        "valos:defaultCoupledField": "valos:ownlings",
        "rdfs:comment": "The owner of this Resource"
      },
      "name": {
        "@type": "valos:PrimaryField",
        "rdfs:domain": "valos:Resource",
        "rdfs:range": "xsd:string",
        "valos:restriction": {
