{
  "@valos/toolset-domain": {
    "inUse": true,
    "domain": "@valos/kernel",
    "workspaces": {
      "@valos/kernel": "./packages/workshop/package",
      "valma": "./packages/valma/package",
      "@valos/engine": "./packages/engine/package",
      "@valos/gateway-api": "./packages/gateway-api/package",
      "@valos/hypertwin": "./packages/hypertwin/package",
      "@valos/inspire": "./packages/inspire/package",
      "@valos/raem": "./packages/raem/package",
      "@valos/revdoc": "./packages/revdoc/package",
      "@valos/sbomdoc": "./packages/sbomdoc/package",
      "@valos/script": "./packages/script/package",
      "@valos/sourcerer": "./packages/sourcerer/package",
      "@valos/tools": "./packages/tools/package",
      "@valos/toolset-domain": "./packages/toolset-domain/package",
      "@valos/toolset-rest-api-gateway-plugin": "./packages/toolset-rest-api-gateway-plugin/package",
      "@valos/toolset-revealer": "./packages/toolset-revealer/package",
      "@valos/twindoc": "./packages/twindoc/package",
      "@valos/type-gateway-plugin": "./packages/type-gateway-plugin/package",
      "@valos/type-opspace": "./packages/type-opspace/package",
      "@valos/type-vault": "./packages/type-vault/package",
      "@valos/type-worker": "./packages/type-worker/package",
      "@valos/vdoc": "./packages/vdoc/package"
    },
    "tools": {
      "summary": {
        "inUse": true,
        "regenerateOnRelease": true,
        "summaryTarget": "revdocs/domain-summary.json"
      }
    }
  },
  "@valos/toolset-revealer": {
    "inUse": true,
    "webpack": {
      "entry": [
        "./packages/inspire/index.js"
      ],
      "output": {
        "path": "dist/revealer/valos/inspire/",
        "publicPath": "/",
        "filename": "valos-inspire.js"
      }
    },
    "tools": {}
  },
  "@valos/type-vault": {
    "tools": {
      "docs": {
        "docsBaseIRI": "https://valospace.org/",
        "regenerateOnRelease": true,
        "authors": {
          "iridian": {
            "name": "Iridian Kiiskinen",
            "url": "https://valaatech.github.io/fabric/authors/iridian",
            "github": "http://github.com/valospace"
          },
          "Jaradacl": {
            "name": "Jaradacl",
            "url": "https://valaatech.github.io/fabric/authors/Jaradacl",
            "github": "http://github.com/valospace"
          }
        },
        "introduction": [],
        "inUse": true
      }
    },
    "inUse": "always"
  }
}
