{
  "@valos/toolset-domain": {
    "inUse": true,
    "domain": "@valos/kernel",
    "workspaceIds": {
      "@valos/kernel": "./packages/workshop",
      "valma": "./packages/valma",
      "@valos/engine": "./packages/engine",
      "@valos/gateway-api": "./packages/gateway-api",
      "@valos/hypertwin": "./packages/hypertwin",
      "@valos/inspire": "./packages/inspire",
      "@valos/raem": "./packages/raem",
      "@valos/revdoc": "./packages/revdoc",
      "@valos/sbomdoc": "./packages/sbomdoc",
      "@valos/script": "./packages/script",
      "@valos/sourcerer": "./packages/sourcerer",
      "@valos/tools": "./packages/tools",
      "@valos/toolset-domain": "./packages/toolset-domain",
      "@valos/toolset-rest-api-gateway-plugin": "./packages/toolset-rest-api-gateway-plugin",
      "@valos/toolset-revealer": "./packages/toolset-revealer",
      "@valos/twindoc": "./packages/twindoc",
      "@valos/type-gateway-plugin": "./packages/type-gateway-plugin",
      "@valos/type-opspace": "./packages/type-opspace",
      "@valos/type-vault": "./packages/type-vault",
      "@valos/type-worker": "./packages/type-worker",
      "@valos/vdoc": "./packages/vdoc"
    },
    "tools": {
      "summary": {
        "inUse": true,
        "regenerateOnRelease": true,
        "target": "revdocs/domain-summary.json"
      }
    }
  },
  "@valos/toolset-revealer": {
    "inUse": true,
    "webpack": {
      "entry": [
        "./packages/inspire/index.js"
      ],
      "output": {
        "path": "dist/revealer/valos/inspire/",
        "publicPath": "/",
        "filename": "valos-inspire.js"
      }
    },
    "tools": {}
  },
  "@valos/type-vault": {
    "tools": {
      "docs": {
        "inUse": true,
        "docsBaseIRI": "https://valospace.org/",
        "regenerateOnRelease": true,
        "authors": {
          "iridian": {
            "name": "Iridian Kiiskinen",
            "url": "https://valaatech.github.io/fabric/authors/iridian",
            "github": "http://github.com/valospace"
          },
          "Jaradacl": {
            "name": "Jaradacl",
            "url": "https://valaatech.github.io/fabric/authors/Jaradacl",
            "github": "http://github.com/valospace"
          }
        },
        "introduction": [],
        "listingTarget": "revdocs/documents-summary.json"
      }
    },
    "inUse": "always"
  }
}
