{
  "@valos/toolset-revealer": {
    "webpack": {
      "entry": [
        "./packages/inspire/index.js"
      ],
      "output": {
        "path": "dist/revealer/valos/inspire/",
        "publicPath": "/",
        "filename": "valos-inspire.js"
      }
    }
  },
  "@valos/toolset-vault": {
    "revdoc": {
      "docs": ["vdoc", "revdoc", "sbomdoc"],
      "editors": {
        "iridian": {
          "name": "Iridian Kiiskinen",
          "url": "https://valaatech.github.io/fabric/editors/iridian",
          "github": "http://github.com/valospace"
        }
      }
    },
    "inUse": false
  },
  "@valos/toolset-vault-sbom": {
    "inUse": true,
    "commands": {
      ".configure/.type/.vault/.toolset/@valos/toolset-vault-sbom": {
        "release-regenerate": true
      }
    },
    "documentURIBase": "https://valaatech.github.io/vault/",
    "introduction": []
  }
}
